    "autoload-dev": {
        "psr-4": {
            "Xbigdaddyx\\HarmonyFlow\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xbigdaddyx\\HarmonyFlow\\HarmonyFlowServiceProvider"
            ],
            "aliases": {
                "HarmonyFlow": "Xbigdaddyx\\HarmonyFlow\\Facades\\HarmonyFlow"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
