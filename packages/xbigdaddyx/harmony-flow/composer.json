    "require": {
        "filament/filament": "^3.0",
        "spatie/laravel-model-status": "^1.14.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "spatie/laravel-permission": "^6.1",
        "ringlesoft/laravel-process-approval": "^1.0.5",
        "illuminate/contracts": "^10.0"
