{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "alperenersoy/filament-export": "v3.0.3-beta",
        "ariaieboy/filament-currency": "^1.1",
        "awcodes/filament-badgeable-column": "^2.2",
        "awcodes/shout": "^2.0",
        "aymanalhattami/filament-slim-scrollbar": "^2.0",
        "barryvdh/laravel-dompdf": "^2.0",
        "bezhansalleh/filament-language-switch": "^2.0",
        "bezhansalleh/filament-panel-switch": "^1.0",
        "bezhansalleh/filament-shield": "^3.0",
        "davidhsianturi/blade-bootstrap-icons": "^1.4",
        "directorytree/ldaprecord-laravel": "^3.1",
        "eightynine/filament-approvals": "^3.0",
        "filament/filament": "^3.0",
        "filament/notifications": "^3.0",
        "filament/spatie-laravel-media-library-plugin": "^3.0-stable",
        "filament/tables": "^3.0-stable",
        "guzzlehttp/guzzle": "^7.2",
        "hasnayeen/themes": "^3.0",
        "jantinnerezo/livewire-alert": "^3.0",
        "jeffgreco13/filament-breezy": "^2.1",
        "laravel/framework": "^10.10",
        "laravel/horizon": "^5.21",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.9",
        "laravel/telescope": "^4.17",
        "laravel/tinker": "^2.8",
        "leandrocfe/filament-apex-charts": "^3.0",
        "livewire/livewire": "^3.0",
        "livewire/volt": "^1.0",
        "pboivin/filament-peek": "^2.0",
        "predis/predis": "^2.2",
        "richan-fongdasen/eloquent-blameable": "^1.9",
        "rickdbcn/filament-email": "^1.0",
        "ringlesoft/laravel-process-approval": "^1.0.5",
        "robsontenorio/mary": "^0.43.0",
        "ryangjchandler/blade-tabler-icons": "^2.1",
        "saade/filament-autograph": "^3.0",
        "sfolador/laravel-locked": "^0.5.1",
        "socialiteproviders/microsoft-azure": "^5.1",
        "socialiteproviders/zoho": "^4.1",
        "spatie/laravel-permission": "^6.1",
        "stechstudio/filament-impersonate": "^3.5",
        "stephenjude/filament-debugger": "^2.0",
        "stijnvanouplines/blade-country-flags": "^1.0",
        "tapp/filament-auditing": "^3.0",
        "teresa/carton-box-guard": "^1.0",
        "xbigdaddyx/harmony-flow": "dev-master",
        "ysfkaya/filament-phone-input": "^2.1"
    },
    "require-dev": {
        "akrillia/laravel-beyond": "^6.0",
        "barryvdh/laravel-debugbar": "^3.9",
        "fakerphp/faker": "^1.9.1",
        "jeroen-g/laravel-packager": "^2.9",
        "laravel/breeze": "^1.24",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Domain\\": "src/Domain/",
            "Support\\": "src/Support/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": {
        "xbigdaddyx/harmony-flow": {
            "type": "path",
            "url": "/var/www/core/packages/xbigdaddyx/harmony-flow",
            "options": {
                "symlink": true
            }
        }
    }
}
